@model testuser.Models.SysModel.tblEstadosRegistros

@{
    ViewBag.Title = "Details";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<div class="row">
    <div class="col-sm-3"></div>
    <div class="col-sm-6">
        <div class="box box-primary">
            <div class="box-header">
                <h4 class="box-title text-info"><strong>Detalles de "Estado cliente/personal"</strong></h4>
            </div>

            <div class="box-body">
                <div>
                    <hr style="margin-top:-10px;"/>
                    <dl class="dl-horizontal">
                        <dt>
                            @Html.DisplayNameFor(model => model.Estado)
                        </dt>

                        <dd>
                            @Html.DisplayFor(model => model.Estado)
                        </dd>

                        </dl>
                </div>
                <p class="col-lg-offset-1">
                    @Html.ActionLink("Editar", "Edit", new { id = Model.Id_EstadoRegistro },new{ @class="btn bg-orange"}) |
                    @Html.ActionLink("Regresar", "Index",new { },new { @class="btn btn-default"})
                </p>
            </div>
        </div>
    </div>
    <div class="col-sm-3"></div>
</div>


