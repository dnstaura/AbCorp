@model testuser.Models.SysModel.tblMunicipios

@{
    ViewBag.Title = "Details";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<style>
    .shadow {
        -webkit-box-shadow: 0px 0px 14px 0px rgba(0,0,0,0.40);
        -moz-box-shadow: 0px 0px 14px 0px rgba(0,0,0,0.40);
        box-shadow: 0px 0px 14px 0px rgba(0,0,0,0.40);
    }
</style>
<div class="row">
    <div class="col-lg-offset-4 col-sm-4">
        <div class="box box-primary shadow">
            <div class="box-header">
                <h4 class="box-title text-info">Detalles de municipios</h4>
            </div>
            <div class="box-body">
                <div>
                    <hr style="margin-top:-10px;"/>
                    <dl class="dl-horizontal">
                        <dt>
                            @Html.DisplayNameFor(model => model.tblDepartamentos.Departamento)
                        </dt>

                        <dd>
                            @Html.DisplayFor(model => model.tblDepartamentos.Departamento)
                        </dd>

                        <dt>
                            @Html.DisplayNameFor(model => model.Municipio)
                        </dt>

                        <dd>
                            @Html.DisplayFor(model => model.Municipio)
                        </dd>

                    </dl>
                </div>
                <p>
                    @Html.ActionLink("Editar", "Edit", new { id = Model.id_Municipio },new { @class="btn bg-orange"}) |
                    @Html.ActionLink("Regresar", "Index",new { },new { @class="btn btn-default"})
                </p>

            </div>
        </div>
    </div>
</div>

